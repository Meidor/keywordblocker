{
    "name": "keywordblocker",
    "version": "2.2.1",
    "description": "Web Extension to block unwanted videos and channels",
    "scripts": {
        "test": "jest",
        "e2e": "jest --runInBand --config=jest.e2e.config.js",
        "dev": "webpack --mode development --watch",
        "dist": "webpack --mode production",
        "firefox": "web-ext run -s dist --firefox=\"C:\\Program Files\\Firefox Developer Edition\\firefox.exe\"",
        "firefox:build": "web-ext build -s dist",
        "lint": "tsc --noEmit && eslint '*/**/*.{js,ts,tsx}' --quiet --fix"
    },
    "husky": {
        "hooks": {
            "pre-commit": "tsc --noEmit && lint-staged"
        }
    },
    "lint-staged": {
        "*.{js,ts,tsx}": [
            "eslint --fix",
            "git add"
        ]
    },
    "sideEffects": [
        "**/*.css",
        "**/*.scss"
    ],
    "author": "Rik Harink",
    "license": "MIT",
    "devDependencies": {
        "@types/jest": "25.1.1",
        "@types/puppeteer": "^2.0.0",
        "@typescript-eslint/eslint-plugin": "2.19.0",
        "@typescript-eslint/parser": "2.19.0",
        "clean-webpack-plugin": "3.0.0",
        "copy-webpack-plugin": "5.1.1",
        "css-loader": "3.4.2",
        "eslint": "6.8.0",
        "eslint-config-prettier": "6.10.0",
        "eslint-plugin-prettier": "3.1.2",
        "html-loader": "0.5.5",
        "html-webpack-plugin": "3.2.0",
        "husky": "4.2.1",
        "jest": "25.1.0",
        "jest-puppeteer": "^4.4.0",
        "lint-staged": "10.0.7",
        "mini-css-extract-plugin": "0.9.0",
        "node-sass": "4.13.1",
        "optimize-css-assets-webpack-plugin": "5.0.3",
        "postcss-cssnext": "3.1.0",
        "postcss-loader": "3.0.0",
        "prettier": "1.19.1",
        "puppeteer": "^2.1.0",
        "sass-loader": "8.0.2",
        "source-map-loader": "0.2.4",
        "terser-webpack-plugin": "2.3.4",
        "ts-jest": "25.2.0",
        "ts-loader": "6.2.1",
        "typescript": "3.7.5",
        "web-ext": "4.0.0",
        "webpack": "4.41.5",
        "webpack-bundle-analyzer": "3.6.0",
        "webpack-cli": "3.3.10",
        "webpack-extension-reloader": "1.1.4"
    },
    "dependencies": {
        "@fortawesome/fontawesome": "1.1.8",
        "@fortawesome/fontawesome-free-brands": "5.0.13",
        "@fortawesome/fontawesome-free-regular": "5.0.13",
        "@fortawesome/fontawesome-free-solid": "5.0.13",
        "@fortawesome/fontawesome-svg-core": "1.2.26",
        "@fortawesome/free-regular-svg-icons": "5.12.0",
        "@fortawesome/free-solid-svg-icons": "5.12.0",
        "@types/expect-puppeteer": "^4.4.0",
        "@types/jest-environment-puppeteer": "^4.3.1",
        "@types/node": "^13.7.0",
        "chrome-extension-async": "git://github.com/KeithHenry/chromeExtensionAsync.git",
        "formbase": "12.0.0",
        "normalize-scss": "7.0.1",
        "rxjs": "6.5.4",
        "ts-debounce": "2.0.1"
    },
    "browserslist": [
        "last 2 Chrome versions",
        "last 2 Firefox versions",
        "last 2 FirefoxAndroid versions",
        "last 2 Opera versions"
    ]
}
